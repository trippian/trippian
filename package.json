{
  "name": "trippian",
  "description": "your local travel companion ",
  "main": "src/entry.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 60 --branches 60 --functions 10 --lines 60",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "dev": "parallelshell 'npm run test' 'npm run start'",
    "copydist": "gulp copy",
    "lint": "eslint src",
    "lint-server": "eslint server",
    "lint:fix": "eslint --fix src",
    "lint-server:fix": "eslint --fix server",
    "test": "mocha 'src/**/*.spec.js' -w --compilers js:babel-core/register",
    "test:server": "mocha 'server/**/*.spec.js' --compilers js:babel-core/register",
    "test:serverw": "mocha 'server/**/*.spec.js' -w --compilers js:babel-core/register",
    "test:single": "istanbul cover -x *.spec.js _mocha -- -R spec 'src/**/*.spec.js' --compilers js:babel-core/register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "webpack-server": "webpack-dev-server --hot --progress --colors",
    "web-server": "http-server -p 3000 ./dist",
    "build": "NODE_ENV=production webpack && gulp copyd",
    "trans-component": " babel-node --plugins react-intl --presets es2015,stage-1,react src/client/components/**/*.js",
    "trans-container": " babel-node --plugins react-intl --presets es2015,stage-1,react src/client/containers/**/*.js",
    "trans-extract": " babel-node --plugins react-intl --presets es2015,stage-1,react translate/script/translate.js",
    "translate": "npm run trans-component && npm run trans-container && npm run trans-extract",
    "start": "npm run webpack-server & npm run web-server",
    "start:server": "nodemon server/src/server.js --exec babel-node --presets es2015"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trippian/trippian.git"
  },
  "keywords": [
    "trippian",
    "travel"
  ],
  "author": "Trippian Team: Audrey, Elliot, Joe, Yale(https://github.com/trippian)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trippian/trippian/issues"
  },
  "homepage": "https://github.com/trippian/trippian#readme",
  "dependencies": {
    "aws-sdk": "^2.2.30",
    "babel-cli": "^6.4.5",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "cors": "^2.7.1",
    "dotenv": "2.0.0",
    "es6-promise": "3.0.2",
    "express": "^4.13.3",
    "express-graphql": "^0.4.5",
    "express-session": "^1.13.0",
    "fetch": "1.0.0",
    "graphql": "^0.4.14",
    "hello-international": "2.2.0",
    "history": "1.17.0",
    "immutable": "^3.7.6",
    "immutable-js": "0.3.1-6",
    "intl-messageformat-parser": "1.2.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.0.0",
    "lru-memoize": "1.0.0",
    "morgan": "^1.6.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "passport-google-oauth": "^0.2.0",
    "path": "^0.12.7",
    "react": "0.14.6",
    "react-bootstrap": "0.28.2",
    "react-dom": "0.14.6",
    "react-intl": "2.0.0-beta-2",
    "react-redux": "4.0.6",
    "react-router": "1.0.3",
    "redux": "3.0.5",
    "redux-form": "^4.1.5",
    "redux-logger": "2.4.0",
    "redux-thunk": "1.0.3",
    "seraph": "^0.14.0",
    "sort-object": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-cli": "^6.4.5",
    "babel-core": "5.8.34",
    "babel-eslint": "4.1.6",
    "babel-loader": "5.4.0",
    "babel-plugin-react-intl": "2.0.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "browser-sync": "2.11.0",
    "chai": "3.4.1",
    "chai-equal-jsx": "1.0.9",
    "chai-immutable": "1.5.3",
    "classnames": "2.2.3",

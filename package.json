    "parallelshell": "2.0.0",
    "postcss-loader": "^0.5.1",
    "protractor": "3.0.0",
    "react-addons-test-utils": "0.14.6",
    "react-hot-loader": "1.3.0",
    "redux": "3.0.5",
    "redux-devtools": "3.0.1",
    "sass-loader": "^2.0.1",
    "semantic-release": "^4.3.5",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.12.3",
    "supertest": "1.1.0",
    "svgo": "^0.5.6",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.6",
    "webpack": "1.12.11",
    "webpack-dev-server": "^1.10.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run test:server && npm run check-coverage"
    }
  },
  "engines": {
    "node": "4.1.1"
  }
}
